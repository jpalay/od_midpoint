{% extends "base.html.tmp" %}
{% block title %}Josh Palay{% endblock %}
{% block css %}
	<link rel="stylesheet" type="text/css" href="css/gallery.css">
{% endblock %}

{% block js %}
	<script type="text/javascript" src="js/gallery.js"></script>
{% endblock %}

{% block content %}
<div id="gallery" class="info_box">
	<h3>some less webby things i've done:</h3>
	<div class="image" id="israel">
		<img src="img/gallery/thumbs/israel.jpg" alt="View from Rosh Pinna, Israel">
	</div>

	<div class="image" id="merge">
		<img src="img/gallery/thumbs/merge.jpg" alt="Buffalo skull in the streets of Engalnd">
	</div>

	<div class="image" id="frost">
		<img src="img/gallery/thumbs/frost.jpg" alt="Rotoscope of Robert Frost">
	</div>

	<div class="image" id="cell1">
		<img src="img/gallery/thumbs/cell1.jpg" alt="Longitudinal section of a plant cell under a microscope">
	</div>

	<div class="image" id="cell2">
		<img src="img/gallery/thumbs/cell2.jpg" alt="Cross section of a plant cell under a microscope">
	</div>

	<div class="image" id="flies">
		<img src="img/gallery/thumbs/flies.jpg" alt="Flies mating">
	</div>

	<div class="image" id="jasper">
		<img src="img/gallery/thumbs/jasper.jpg" alt="Dog sleeping under a table">
	</div>
</div>
{% endblock %}

{% block other_html %}
<div id="lightbox">
	<div id="light_content">
	</div>
</div>
{% endblock %}